@{
    ViewBag.Title = "GroupWeekIndex";
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">

    </script>
}

<div class="panel">
    <div class="panel-heading">
        <a class="btn btn-success thickbox" title='添加新用户' href="@Url.Action("GroupWeekCreate")?TB_iframe=true&height=450&width=500"><i class="fa fa-plus" aria-hidden="true"></i> 新增</a>

    </div>
        <div class="panel-body">
            <table class="table table-striped table-hover ">

                <thead>
                    <tr>
                        <th>序号</th>
                        <th>组号</th>
                        <th>时间</th>
                        <th>是否是优秀团队</th>
                        <th>操作</th>
                    </tr>
                </thead>
                <tbody>
                    @if (ViewBag.Data == null)
                    {

                    }
                    else
                    {
                        int i = 1;
                        foreach (ShiNengShiHui.AppServices.WeekGradeDTO.GroupWeekGradeShow m in ViewBag.Data as List<ShiNengShiHui.AppServices.WeekGradeDTO.GroupWeekGradeShow>)
                        {
                            <tr>
                                <td>@i</td>
                                <td>@m.Group</td>
                                <td>@(m.Date.SchoolYear+"学年  "+m.Date.Semester+"学期  第"+m.Date.Week+"周")</td>
                                <td>
                                    @if (m.IsWell)
                                    {
                                        <span>是</span>
                                    }
                                    else
                                    {
                                        <span>否</span>
                                    }
                                </td>
                                <td>
                                    <a class="btn btn-info thickbox" title='编辑' href="@Url.Action("GroupWeekUpdate", new { id = m.Id })?TB_iframe=true&height=450&width=500">
                                        <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                        编辑
                                    </a>
                                </td>
                            </tr>
                            i++;
                        }
                    }


                </tbody>
            </table>
        </div>
</div>
